{;; Specify that this is a Clojure-only library
 :cljdoc/languages [:clj]

 ;; Documentation tree structure
 ;; Note: Optional namespaces (tools.malli, tools.schema) won't have API docs
 ;; on cljdoc since malli/schema are optional dependencies.
 ;; These features are documented in TOOLS.md instead.
 :cljdoc.doc/tree [["Readme" {:file "README.md"}]
                   ["Getting Started"
                    ["Installation" {:file "README.md"}]
                    ["Quick Start" {:file "README.md"}]]
                   ["Guides"
                    ["Tools & Function Calling" {:file "docs/TOOLS.md"}]
                    ["Resilience & Failover" {:file "docs/RESILIENCE.md"}]]
                   ["Reference"
                    ["Changelog" {:file "CHANGELOG.md"}]
                    ["Contributing" {:file "CONTRIBUTING.md"}]]]}
