{:paths ["src" "resources"]
 :deps {org.clojure/clojure {:mvn/version "1.12.1"}
        org.clojure/data.json {:mvn/version "2.5.0"}
        org.clojure/tools.logging {:mvn/version "1.3.0"}
        dev.langchain4j/langchain4j-core {:mvn/version "1.9.1"}
        dev.langchain4j/langchain4j {:mvn/version "1.9.1"}
        dev.langchain4j/langchain4j-open-ai {:mvn/version "1.9.1"}
        dev.langchain4j/langchain4j-anthropic {:mvn/version "1.9.1"}
        dev.langchain4j/langchain4j-mistral-ai {:mvn/version "1.9.1"}
        dev.langchain4j/langchain4j-google-ai-gemini {:mvn/version "1.9.1"}
        dev.langchain4j/langchain4j-vertex-ai-gemini {:mvn/version "1.9.1-beta17"}
        dev.langchain4j/langchain4j-ollama {:mvn/version "1.9.1"}
        dev.langchain4j/langchain4j-document-parser-apache-tika {:mvn/version "1.9.1-beta17"}
        metosin/jsonista {:mvn/version "0.3.10"}}
 :aliases
 {:run-m {:main-opts ["-m" "langchain4clj"]}
  :run-x {:ns-default langchain4clj
          :exec-fn greet
          :exec-args {:name "Clojure"}}
  :build {:deps {io.github.clojure/tools.build {:mvn/version "0.10.10"}
                 slipset/deps-deploy {:mvn/version "0.2.2"}}
          :ns-default build}
  :test {:extra-paths ["test"]
         :extra-deps {org.clojure/test.check {:mvn/version "1.1.1"}
                      io.github.seancorfield/test-runner {:git/tag "v0.6.0" :git/sha "d5f18c5"}}
         :main-opts ["-m" "cognitect.test-runner" "-e" ":integration"]
         :exec-fn cognitect.test-runner.api/test
         :exec-args {:excludes [:integration]}}
  :test-integration {:extra-paths ["test"]
                     :extra-deps {org.clojure/test.check {:mvn/version "1.1.1"}
                                  io.github.seancorfield/test-runner {:git/tag "v0.6.0" :git/sha "d5f18c5"}}
                     :main-opts ["-m" "cognitect.test-runner" "-i" ":integration"]
                     :exec-fn cognitect.test-runner.api/test
                     :exec-args {:includes [:integration]}}
  :nrepl {:extra-paths ["test"]
          :extra-deps {nrepl/nrepl {:mvn/version "1.3.1"}}
          ;; this allows nrepl to interrupt runaway repl evals
          :jvm-opts ["-Djdk.attach.allowAttachSelf"]
          :main-opts ["-m" "nrepl.cmdline" "--port" "7888"]}

  :with-schema {:extra-deps {prismatic/schema {:mvn/version "1.4.1"}}}

  :with-malli {:extra-deps {metosin/malli {:mvn/version "0.16.4"}}}

  :with-all-schemas {:extra-deps {prismatic/schema {:mvn/version "1.4.1"}
                                  metosin/malli {:mvn/version "0.16.4"}}}

  ;; For development with all features
  :dev {:extra-paths ["dev" "test" "examples"]
        :extra-deps {prismatic/schema {:mvn/version "1.4.1"}
                     metosin/malli {:mvn/version "0.16.4"}
                     org.clojure/test.check {:mvn/version "1.1.1"}}}

  :lint {:extra-deps {clj-kondo/clj-kondo {:mvn/version "2024.11.14"}}
         :main-opts ["-m" "clj-kondo.main" "--lint" "src" "test"]}}}

